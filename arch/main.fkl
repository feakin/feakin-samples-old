include "./config"
include "./context-reservation"
include "./guarding-layered"

ContextMap TicketBooking {
    Reservation -> Cinema;
    Reservation -> Movie;
    Reservation <-> User;
}

SourceSet sourceSet {
    feakin {
        srcDir: ["src/main/resources/uml"]
    }
    puml {
        parser: "PlantUML"
        srcDir: ["src/main/resources/uml"]
    }
}

impl PackageJsonGet {
    endpoint {
        GET "https://raw.githubusercontent.com/feakin/vscode-feakin/master/package.json";
    }
}
